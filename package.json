{
	"name": "compendium-devtools",
	"version": "0.0.0",
	"description": "A framework-agnostic devtools extension made for SolidHack.",
	"info": "Why should everything need to build a devtools extension? Compendium-Devtools's goal is to save framework developers and their users time.",
	"main": "index.js",
	"scripts": {
		"dev": "vite build --watch",
		"dev-ui": "cd src/ui && npm run build -- -w",
		"dev-start": "cd dist && web-ext run --target=chromium --chromium-binary=\"C:/Program Files/Vivaldi/Application/vivaldi.exe\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CompendiumDevTools/devtools.git"
	},
	"keywords": [
		"solidhack",
		"best_ecosystem",
		"devtools",
		"store",
		"state",
		"extension",
		"inspector"
	],
	"author": "Kyza",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CompendiumDevTools/devtools/issues"
	},
	"homepage": "https://github.com/CompendiumDevTools/devtools#readme",
	"devDependencies": {
		"@types/chrome": "^0.0.178",
		"@types/node": "^17.0.21",
		"solid-js": "^1.3.13",
		"typescript": "^4.6.2",
		"vite": "^2.8.6"
	},
	"dependencies": {
		"@solid-primitives/resize-observer": "^1.0.5",
		"solid-app-router": "^0.3.1"
	}
}
